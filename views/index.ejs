<%- include('_partials/_layout-start') %> 
<% data.forEach(datum => { %>
  <div class="card" style="width: 18rem;">
    <img src="<%= datum.picture %>" class="card-img-top" alt="...">
    <div class="card-body">
      <h5 class="card-title"><%= datum.title %></h5>
      <p class="card-text"><%= datum.subtitle %></p>
      <a href="#" class="btn btn-primary">Go somewhere</a>
    </div>
  </div>
<% }) %>
<%- include('_partials/_layout-end') %> 
